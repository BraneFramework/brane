import v6_client;
import v6_node;
import v6_server;

let s := server(5000);
s.waitUntilStarted();

let session := login("root", "root");
let token := session.access_token;

let collaboration := create_collaboration(token, "collab", "1");
let node := create_node(token, collaboration.id);

let n := node(node.api_key, s.address, 5000);
n.waitUntilStarted();

